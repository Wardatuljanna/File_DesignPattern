<!DOCTYPE html>
<html>
<head>
  <title>Movie List</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 20px;
    }
    h1 {
      color: #333;
    }
    table {
      border-collapse: collapse;
      width: 80%;
      margin: 20px auto;
    }
    th, td {
      border: 1px solid #333;
      padding: 10px;
      text-align: left;
    }
    th {
      background-color: #333;
      color: #fff;
    }
    img {
      max-width: 100px;
      max-height: 150px;
    }
  </style>
</head>
<body>
  <h1>Movie List</h1>
  <table>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Genres</th>
      <th>Year</th>
      <th>Photo</th>
    </tr>
    <% movies.forEach(function(movie) { %>
      <tr>
        <td><%= movie.id %></td>
        <td><%= movie.title %></td>
        <td><%= movie.genres %></td>
        <td><%= movie.year %></td>
        <td><img src="/upload/<%= movie.photo %>" width="100" height="150"></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>
